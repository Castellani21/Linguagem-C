/* Leia quatro notas, calcule a média aritmética e imprima o resultado*/

#include <stdio.h>
int main(){

    float a, b, c, d, media;

    printf("Digite a suas notas ");
            scanf("%d %d %d %d", &a , &b , &c, &d);
        
        media = (a + b + c + d)/4;

        printf("Sua nota foi de %d", media);

        return 0;
}
